<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:context="http://www.springframework.org/schema/context"
		xmlns:p="http://www.springframework.org/schema/p"
		xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/context
		http://www.springframework.org/schema/context/spring-context.xsd">
		
		
	<bean name="emp1" class="com.springcore.collections.Employee">
	
	    <property name="empName" value="Tulsi Kumari"/>
	    
	    
	    <property name="empContact">
	    	<list>
	    	   <value>6202307934</value>
	    	   <value>9031043600</value>
	    	   <value>9608154653</value>
	    	</list>
	    </property>
	    
	    
	    <property name="empAdd">
	    	<set>
	    		<value>Jamshedpur</value>
	    		<value>Patna</value>
	    		<value>Salempur</value>
	    	</set>
	    </property>
	    
	    
	    <property name="empCourses">
	    	<map>
	    		<entry key="java" value="3months"/>
	    		<entry key="python" value="1month"/>
	    		<entry key="solidity" value="0.5month"/>
	    	</map>
	    </property>
	    
	</bean>
	
	
	
</beans>