<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:context="http://www.springframework.org/schema/context"
		xmlns:p="http://www.springframework.org/schema/p"
		xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/context
		http://www.springframework.org/schema/context/spring-context.xsd">
		
	<context:annotation-config/>	
	<bean name="p1" class="com.springcore.lifecycleMethods.Person" init-method="init" destroy-method="destroy">
	
	    <property name="name" value="Tulsi Kumari"/>
	    
	</bean>
	<bean name="p2" class="com.springcore.lifecycleMethods.UsingInterface">
	
	    <property name="name" value="Tulsi Kumari"/>
	    
	</bean>
	<bean name="p3" class="com.springcore.lifecycleMethods.UsingAnnotation">
	
	    <property name="name" value="Tulsi Kumari"/>
	    
	</bean>
	
	
</beans>